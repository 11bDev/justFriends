{
  "manifest_version": 3,
  "name": "Just Friends - Facebook Friends Feed Redirect",
  "version": "1.0.0",
  "description": "Automatically redirects to your Facebook friends feed when opening Facebook",
  "permissions": [],
  "host_permissions": [
    "*://*.facebook.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
